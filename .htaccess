RewriteRule ^mobile$ ./app.php?func=mobile$1 [QSA,L]
RewriteRule ^search_control$ ./app.php?func=search_control$1 [QSA,L]
RewriteRule ^stats$ ./statistics.php$1 [QSA,L]
